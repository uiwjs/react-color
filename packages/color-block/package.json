{
  "name": "@uiw/react-color-block",
  "version": "2.8.0",
  "description": "Color Block Picker",
  "author": "Kenny Wong <wowohoo@qq.com>",
  "homepage": "https://uiwjs.github.io/react-color/#/block",
  "funding": "https://jaywcjlove.github.io/#/sponsor",
  "sideEffects": false,
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
