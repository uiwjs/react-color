  "license": "MIT",
  "files": [
    "esm",
    "cjs",
    "src"
  ],
  "peerDependencies": {
    "@babel/runtime": ">=7.19.0",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "dependencies": {
    "@uiw/color-convert": "2.8.0",
    "@uiw/react-color-editable-input": "2.8.0",
    "@uiw/react-color-swatch": "2.8.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.21.0"
  }
