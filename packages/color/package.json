{
  "name": "@uiw/react-color",
  "version": "2.9.2",
  "description": "Color Picker component for React.",
  "homepage": "https://uiwjs.github.io/react-color",
  "funding": "https://jaywcjlove.github.io/#/sponsor",
  "author": "Kenny Wong <wowohoo@qq.com>",
  "sideEffects": false,
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "exports": {
    ".": {
