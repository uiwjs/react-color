    "@uiw/react-color-editable-input-rgba": "workspace:*",
    "@uiw/react-color-github": "workspace:*",
    "@uiw/react-color-hue": "workspace:*",
    "@uiw/react-color-material": "workspace:*",
    "@uiw/react-color-name": "workspace:*",
    "@uiw/react-color-saturation": "workspace:*",
    "@uiw/react-color-shade-slider": "workspace:*",
    "@uiw/react-color-sketch": "workspace:*",
    "@uiw/react-color-slider": "workspace:*",
    "@uiw/react-color-swatch": "workspace:*",
    "@uiw/react-color-wheel": "workspace:*"
  },
  "devDependencies": {
    "@babel/runtime": "^7.21.0"
  }
