    "url": "https://github.com/uiwjs/react-color.git"
  },
  "scripts": {
    "build": "tsbb build src/*.tsx --use-babel --cjs cjs",
    "watch": "tsbb watch src/*.tsx --use-babel --cjs cjs"
  },
  "keywords": [
    "react",
    "react-color",
    "react-component",
    "color-picker",
    "colorpicker",
    "color picker",
    "colorful",
    "color",
    "react-color-alpha",
    "react-color-hua",
    "color-convert",
    "hua",
    "shade-slider",
    "slider",
    "circle",
    "compact",
    "sketch",
    "chrome",
    "alpha",
    "editable-input",
    "convert"
  ],
  "license": "MIT",
  "files": [
    "dist",
    "esm",
    "cjs",
    "src"
  ],
  "peerDependencies": {
    "@babel/runtime": ">=7.19.0",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "dependencies": {
    "@uiw/color-convert": "2.9.2",
    "@uiw/react-color-alpha": "workspace:*",
    "@uiw/react-color-block": "workspace:*",
    "@uiw/react-color-chrome": "workspace:*",
