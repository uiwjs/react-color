{
  "name": "@uiw/color-convert",
  "version": "2.5.1",
  "description": "Color Convert",
  "author": "Kenny Wong <wowohoo@qq.com>",
  "homepage": "https://uiwjs.github.io/react-color/#/convert",
  "funding": "https://jaywcjlove.github.io/#/sponsor",
  "sideEffects": false,
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./esm/index.js"
    },
    "./*": "./*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/uiwjs/react-color.git"
  },
  "scripts": {
    "build": "tsbb build src/*.ts --use-babel --cjs cjs",
    "watch": "tsbb watch src/*.ts --use-babel --cjs cjs"
  },
  "keywords": [
    "color",
    "convert"
  ],
  "license": "MIT",
  "files": [
    "esm",
    "cjs",
    "src"
  ],
  "peerDependencies": {
    "@babel/runtime": ">=7.19.0"
  },
  "devDependencies": {
    "@babel/runtime": "^7.21.0"
  }
}
